pkg update && pkg upgrade
pkg install openjdk-17-jdk -y
wget --no-check-certificate https://github.com/WebGoat/WebGoat/releases/download/v2023.4/webgoat-2023.4.jar
mv webgoat-2023.4.jar $PREFIX/bin
wget --no-check-certificate https://raw.githubusercontent.com/RDXLR/Webgoat-Termux/main/webgoat
chmod 777 webgoat
mv webgoat $PREFIX/bin
